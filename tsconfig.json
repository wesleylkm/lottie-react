{
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["DOM", "ES2016"],
    "jsx": "react-jsxdev",
    "allowSyntheticDefaultImports": true,
    "noEmit": false,
    "moduleResolution": "node",
    "resolveJsonModule": true
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
